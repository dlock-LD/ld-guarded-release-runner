# LaunchDarkly Credentials
SDK_KEY=sdk-xxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
API_KEY=api-xxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
PROJECT_KEY=your-project-key
RG_FLAG_KEY=your-flag-key

# Metrics Configuration
NUMERIC_METRIC_1=latency
BINARY_METRIC_1=error-rate

# Ranges for NUMERIC_METRIC_1 (latency)
# Format: JSON array with [min, max]
NUMERIC_METRIC_1_FALSE_RANGE=[50, 100]
NUMERIC_METRIC_1_TRUE_RANGE=[100, 200]

# Conversion rates for BINARY_METRIC_1 (error-rate)
# Percentage (0-100) of events that will trigger this metric
BINARY_METRIC_1_FALSE_CONVERTED=5
BINARY_METRIC_1_TRUE_CONVERTED=15